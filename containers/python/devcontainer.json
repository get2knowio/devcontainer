{
  "name": "Python Development Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "features": {
    "ghcr.io/devcontainers-extra/features/poetry:2": {
      "version": "latest"
    }
  },
  "postCreateCommand": "poetry --version",
  "remoteUser": "vscode",
  "workspaceFolder": "/workspace"
}
