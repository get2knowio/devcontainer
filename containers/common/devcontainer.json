{
  "name": "DevContainer Base Common Image",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".",
    "args": {
      "BUILDKIT_INLINE_CACHE": "1"
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:1": {
      "version": "latest"
    }
  },
  "runArgs": [
    "--privileged"
  ],
  "overrideCommand": false,
  "init": true,
  "privileged": true,
  "remoteUser": "vscode",
  "workspaceFolder": "/workspace"
}
